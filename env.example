GEMINI_API_KEY=your_gemini_api_key_here

# Get your OpenRouter API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Your Vercel deployment URL (will be auto-filled)
